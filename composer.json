{
    "name": "makaveli/laravel-logger",
    "description": "Advanced Logger for Laravel",
    "keywords": ["laravel", "framework"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.10|^11.0|^12.0",
        "makaveli/laravel-query-builder": "1.0.12"
    },
    "autoload": {
        "psr-4": {
            "Logger\\": "src/",
            "Logger\\Database\\": "src/Database/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ma1kaveli/laravel-query-builder"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Ma1kaveli/laravel-converter"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Logger\\Providers\\LoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
